#!/bin/bash

source ${OPENSHIFT_HOMEDIR}/nginx-node-pm2-issuetracker/bin/util


function build(){
    install_node_app
}

function start(){
    echo "Starting"
}

function stop(){
    echo "stopping"
}

function restart(){
    echo "restarting"
}

case ${1} in
  #pre-repo-archive) pre-repo-archive ;;
    #pre-build)        pre_build        ;;
    build)            build            ;;
    start)            start            ;;
    stop)             stop             ;;
    restart)          restart          ;;
    #status)           status           ;;
    #tidy)             tidy             ;;
    *)                exit 0
esac


